{
  "name": "@argdown/sandbox",
  "version": "1.6.0",
  "private": true,
  "description": "An Argdown editor that runs in the browser",
  "author": "Christian Voigt <1pxsolidblack@googlemail.com>",
  "scripts": {
    "serve": "npm run copy-examples && vue-cli-service serve --open",
    "build": "npm run copy-examples && vue-cli-service build --dest ../../docs/.vuepress/dist/sandbox",
    "lint": "vue-cli-service lint",
    "copy-examples": "rimraf \"./public/examples/*.argdown\" && copyfiles -f \"../../examples/*.argdown\" ./public/examples"
  },
  "dependencies": {
    "@argdown/codemirror-mode": "^1.6.0",
    "@argdown/core": "^1.6.2",
    "@argdown/map-views": "^1.6.2",
    "axios": "^0.21.1",
    "file-saver": "^2.0.2",
    "vue": "^2.6.10",
    "vue-async-computed": "^3.7.0",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@vue/cli-plugin-babel": "^4.0.4",
    "@vue/cli-plugin-eslint": "^4.0.4",
    "@vue/cli-service": "^4.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "copyfiles": "^2.1.1",
    "css-loader": "^5.0.2",
    "eslint": "^7.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^7.6.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.0",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "sideEffects": [
    "./src/main.js",
    "./src/store.js",
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "keywords": [
    "argumentation",
    "argument mapping",
    "Argdown",
    "Editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-sandbox"
  }
}
