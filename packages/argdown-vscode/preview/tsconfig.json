{
    "compilerOptions": {
        "newLine": "LF",
        "outDir": "./dist/",
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    }
}
